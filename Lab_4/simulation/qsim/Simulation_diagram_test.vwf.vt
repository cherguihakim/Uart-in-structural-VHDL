`timescale 1 ps/ 1 ps
module diagram_test_uart_fsm_with_uart_emetteur_vlg_sample_tst(
	CLOCK,
	LOAD_CONTROLEUR,
	LOAD_UART_FSM,
	RESET_BAR,
	STATE,
	sampler_tx
);
input  CLOCK;
input  LOAD_CONTROLEUR;
input  LOAD_UART_FSM;
input  RESET_BAR;
input [1:0] STATE;
output sampler_tx;

reg sample;
time current_time;
always @(CLOCK or LOAD_CONTROLEUR or LOAD_UART_FSM or RESET_BAR or STATE)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

module diagram_test_uart_fsm_with_uart_emetteur_vlg_check_tst (
	o_TxD,
	sampler_rx
);
input  o_TxD;
input sampler_rx;

reg  o_TxD_expected;

reg  o_TxD_prev;

reg  o_TxD_expected_prev;

reg  last_o_TxD_exp;

reg trigger;

integer i;
integer nummismatches;

reg [1:1] on_first_change ;


initial
begin
trigger = 0;
i = 0;
nummismatches = 0;
on_first_change = 1'b1;
end

// update real /o prevs

always @(trigger)
begin
	o_TxD_prev = o_TxD;
end

// update expected /o prevs

always @(trigger)
begin
	o_TxD_expected_prev = o_TxD_expected;
end



// expected o_TxD
initial
begin
	o_TxD_expected = 1'bX;
end 
// generate trigger
always @(o_TxD_expected or o_TxD)
begin
	trigger <= ~trigger;
end

always @(posedge sampler_rx or negedge sampler_rx)
begin
`ifdef debug_tbench
	$display("Scanning pattern %d @time = %t",i,$realtime );
	i = i + 1;
	$display("| expected o_TxD = %b | ",o_TxD_expected_prev);
	$display("| real o_TxD = %b | ",o_TxD_prev);
`endif
	if (
		( o_TxD_expected_prev !== 1'bx ) && ( o_TxD_prev !== o_TxD_expected_prev )
		&& ((o_TxD_expected_prev !== last_o_TxD_exp) ||
			on_first_change[1])
	)
	begin
		$display ("ERROR! Vector Mismatch for output port o_TxD :: @time = %t",  $realtime);
		$display ("     Expected value = %b", o_TxD_expected_prev);
		$display ("     Real value = %b", o_TxD_prev);
		nummismatches = nummismatches + 1;
		on_first_change[1] = 1'b0;
		last_o_TxD_exp = o_TxD_expected_prev;
	end

	trigger <= ~trigger;
end
initial 

begin 
$timeformat(-12,3," ps",6);
#3000000;
if (nummismatches > 0)
	$display ("%d mismatched vectors : Simulation failed !",nummismatches);
else
	$display ("Simulation passed !");
$finish;
end 
endmodule

